#test([2, 7, 11, 15], 9)
micro two_sum(nums: Array<u32>, target: u32) {
    let mut map: HashMap<u32, u32> = [];
    for index, num in nums.indexer() {
        let r = target - num;
        match map.get(r) {
            case Some(s): return (index, s)
            case None: map.insert(num, index)
        }
    };
    return (0, 0)
}