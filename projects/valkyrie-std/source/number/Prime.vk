namespace package.number;

#refine
structure Prime {
    ordinal: Ordinal
}

extends Prime {
    uncheck constructor(n: Natural) -> Self {
        if n == 0 {
            return null
        }
        new Self { ordinal: n }
    }
    constructor(n: Natural) -> Self? {
        if n == 0 {
            return null
        }
        new Self { ordinal: n }
    }
    constructor(n: Integer) -> Self? {
        if n <= 0 {
            return null
        }
        new Self { ordinal: n }
    }
}
