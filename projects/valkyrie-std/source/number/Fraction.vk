namespace package.number;

#native
class Fraction {
    #get(@ffi())
    sign       : Sign  = Sign::NoSign;
    #get(@ffi())
    numerator  : Natural = 0;
    #get(@ffi())
    denominator: Natural = 1;
}

extends Fraction {
    zero() -> Self {
        return 0 as Fraction;
    }
    one() -> Self {
        return 1 as Fraction;
    }
}